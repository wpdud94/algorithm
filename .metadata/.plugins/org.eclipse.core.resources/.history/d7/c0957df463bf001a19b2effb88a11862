package instructors;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collections;
import java.util.Scanner;

public class Algo02_CutSection {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		// 1. 영역 구성
		// 1-1. 가로 세로 한계선 받기
		int height = sc.nextInt();
		int width = sc.nextInt();
		// 2. 가로 세로 별 구분 데이터 추출
		// 2-1. 구분선 개수 받기
		int numOfCut = sc.nextInt();
		int numOfW = 0;
		int numOfH = 0;
		// 2-2. 구분선 모두 받기
		int[]lineArr = new int[numOfCut*2];
		for(int i=0;i<lineArr.length;i++) {
			 lineArr[i] = sc.nextInt();
			 if(i%2==0) {
				 if(lineArr[i]==0) numOfH++;
				 else numOfW++;
			 }
		}
		// 2-3. 0면 가로 구분선 배열에 , 1이면 세로 구분선 배열에 할당... 기본 0 할당 & 한계선 할당
		int[] wLine = new int[numOfW];
		int[] hLine = new int[numOfH];
		int widx = 0;
		int hidx = 0;
		for(int i=0;i<lineArr.length;i=i+2) {
			if(lineArr[i]==0) hLine[hidx++];
			else wLine[widx++];
		}
		hLine.add(width); wLine.add(height);
		Collections.sort(hLine);
		Collections.sort(wLine);
		System.out.println("세로구분선 : " + hLine);
		System.out.println("가로구분선 : " +wLine);
		
		// 2-4. 세로칸, 가로칸 배열 추출
		//세로칸
		
	}
}
/*
10 8
3
0 3
1 4
0 2
*/