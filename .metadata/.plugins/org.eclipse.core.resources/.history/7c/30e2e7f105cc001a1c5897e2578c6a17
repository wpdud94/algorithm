class Solution15 {
	public int[] solution(int n, String[] words) {
        int[] answer = {0,0};
        int turn = 0;
        while(turn<words.length-1) {
        	int index  = (turn % n);
            int round = (turn/n)+1;
            if(words[turn])
            turn++;
        }
        
        
        
        
        return answer;
    }
	
}
